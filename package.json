  "scripts": {
    "start": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "devStart": "node %NODE_DEBUG_OPTION% ./node_modules/react-scripts/scripts/start.js",
    "build_dev": "cross-env REACT_APP_STAGE=dev react-scripts build",
    "build_dev2": "cross-env REACT_APP_STAGE=dev2 react-scripts build --profile",
    "build_dev3": "cross-env REACT_APP_STAGE=dev3 react-scripts build",
    "build_prod": "cross-env REACT_APP_STAGE=prod react-scripts build",
    "build_v2prod": "cross-env REACT_APP_STAGE=v2prod react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "serve": "serve -s build",
    "prettier": "prettier --write \"src/**/*.{css,js}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "browserslist": {
    "development": [
      "ie 11",
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
